<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_select_background"
    android:orientation="vertical"
    tools:context="com.guojianyiliao.eryitianshi.View.activity.LoginSelectActivity">

    <com.guojianyiliao.eryitianshi.page.view.MyViewPager
        android:id="@+id/vp_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y96" />


    <ImageView
        android:layout_width="@dimen/x170"
        android:layout_height="@dimen/y194"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/y96"
        android:src="@drawable/login_select_log" />


    <com.guojianyiliao.eryitianshi.page.view.MyViewPager
        android:id="@+id/vp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
